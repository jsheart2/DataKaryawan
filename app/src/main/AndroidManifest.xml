<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.datakaryawan">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/user"
        android:label="@string/app_name"
        android:roundIcon="@drawable/user"
        android:supportsRtl="true"
        android:theme="@style/Theme.DataKaryawan">
        <activity android:name=".Login"></activity>
        <activity android:name=".Logout"></activity>
        <activity android:name=".Showprofile"></activity>
        <activity android:name=".InputDataKaryawan"></activity>
        <activity android:name=".Dashboard"/>
        <activity android:name=".Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>